{
  "name": "dd-design-center",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "build": "node scripts/generate-posts-json.cjs && vite build",
    "postbuild": "node scripts/run-react-snap.cjs && node scripts/inject-blog-content.js"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "inlineCss": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "concurrency": 4,
    "skipThirdPartyRequests": true,
    "timeout": 300000,
    "include": [
      "/",
      "/blog",
      "/collaboration",
      "/crafted-calm",
      "/designers",
      "/how-we-work",
      "/privacy",
      "/productscollection",
      "/sustainability",
      "/terms"
    ],
    "puppeteer": {
      "cache": false,
      "executablePath": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome"
    },
    "userAgent": "ReactSnap",
    "fixWebpackChunksIssue": false,
    "removeBlobs": true
  },
  "dependencies": {
    "@relume_io/relume-tailwind": "^1.3.0",
    "@relume_io/relume-ui": "^1.3.0",
    "@tanstack/react-virtual": "^3.12.0",
    "@types/react": "^18.x",
    "@types/react-dom": "^18.x",
    "@types/uuid": "^10.0.0",
    "airtable": "^0.12.2",
    "buffer": "^6.0.3",
    "cloudinary": "^2.6.0",
    "clsx": "^2.0.0",
    "container-query-polyfill": "^1.0.2",
    "dotenv": "^16.5.0",
    "framer-motion": "^11.0.8",
    "gsap": "^3.13.0",
    "lucide-react": "^0.501.0",
    "netlify-cms-app": "^2.15.72",
    "netlify-cms-lib-util": "^2.15.1",
    "netlify-cms-media-library-cloudinary": "^1.3.10",
    "react": "^18.x",
    "react-dom": "^18.x",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.5.3",
    "react-markdown": "^10.1.0",
    "react-particles": "^2.12.2",
    "react-router-dom": "^6.22.3",
    "react-snap": "^1.23.0",
    "react-swipeable": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "tailwind-merge": "^1.14.0",
    "tsparticles": "^2.12.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@netlify/plugin-gatsby": "3.8.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/airtable": "^0.10.5",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.1.2",
    "autoprefixer": "10.4.21",
    "cheerio": "1.0.0",
    "cssnano": "7.0.7",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gatsby": "5.14.3",
    "globals": "^15.9.0",
    "gray-matter": "4.0.3",
    "jsdom": "^26.1.0",
    "marked": "15.0.12",
    "netlify-plugin-cache": "1.0.3",
    "node-fetch": "^3.3.2",
    "postcss": "8.5.3",
    "postcss-import": "^16.1.0",
    "puppeteer-core": "24.9.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.34.1",
    "tailwindcss": "3.4.17",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.1.2"
  }
}
